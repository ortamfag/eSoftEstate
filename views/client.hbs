<a class="addClient" href="/addClient">Add client</a>

<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Имя</th>
      <th scope="col">Фамилия</th>
      <th scope="col">Отчество</th>
      <th scope="col">E-mail</th>
      <th scope="col">Телефон</th>
      <th scope="col">Действия</th>
    </tr>
  </thead>

  <tbody>
    {{#each rows}}
    <tr>
      <td>{{this.firstName}}</td>
      <td>{{this.lastName}}</td>
      <td>{{this.patronymic}}</td>
      <td>{{this.email}}</td>
      <td>{{this.phone}}</td>
      <td>
        <a href="/changeClient/{{this.ID}}" type="button">Изменить</a>
        <a href="/deleteClient/{{this.ID}}" type="button">Удаление</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>